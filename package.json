{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React starter kit",
  "main": "webpack-demo.config.js",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "build:webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "clean": "rimraf dist",
    "test": "jest",
    "test:changed": "npm run test -- --changedSince HEAD",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint .",
    "webserver": "http-server dist -p 8082 --cors -c-1",
    "dev": "concurrently --kill-others -n webpack,webserver -c blue,green \"npm run build:webpack-dev -- --watch\" \"npm run webserver\""
  },
  "author": "Jerry Jong",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.6.11",
    "date-fns": "^2.13.0",
    "fs": "0.0.1-security",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "@types/jest": "^25.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^0.12.1",
    "jest": "^25.1.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react-dom": "^16.10.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "styled-components": "^5.1.0",
    "webpack": "^4.41.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
